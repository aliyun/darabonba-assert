{
  "name": "alibabacloud/tea-assert",
  "description": "Alibaba Cloud Tea Assert library for PHP",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [],
  "require": {
    "php": ">5.5",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Alibabacloud\\TeaAssert\\": "src"
    }
  },
  "scripts": {
    "fixer": "php-cs-fixer fix ./"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "prefer-stable": true
}