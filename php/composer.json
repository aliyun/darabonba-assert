{
  "name": "alibabacloud/darabonba-assert",
  "description": "Alibaba Cloud Darabonba Assert library for PHP",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [],
  "require": {
    "php": ">5.5",
    "adbario/php-dot-notation": "^2.2",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Alibabacloud\\DarabonbaAssert\\": "src"
    }
  },
  "scripts": {
    "fixer": "php-cs-fixer fix ./"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "prefer-stable": true
}
